<p style="color: green"><%= notice %></p>
<h1>Items</h1>
<div id="items",class='row'>
  <% @items.each do |item| %>
    <div class='my-box my-4'>
      <%= render item %>
      <p>
        <%= link_to "Show this item", item ,class:'btn btn-primary'%>
      </p>
    </div>
  <% end %>
  <%= link_to "New item", new_item_path ,class:'btn btn-success my-4'%>
</div>
